let arr = [true, "hello", 5, 12, -200, false, null, undefined, "word", 3.14, { x: 1 }, [1, 2, 3]];

let types = {};

for (let i = 0; i < arr.length; i++) {
  let t;
  if (Array.isArray(arr[i])) {
    t = "array";
  } else {
    t = typeof arr[i];
  }

  if (types[t] === undefined) {
    types[t] = 1;
  } else {
    types[t] = types[t] + 1;
  }
}

console.log(types);
